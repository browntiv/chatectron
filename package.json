{
    "name": "electron-chat",
    "version": "0.0.1",
    "main": "main.js",
    "author": "Enes Ozturk <enesozturk.d@gmail.com>",
    "license": "MIT",
    "scripts": {
        "start": "electron main.js --enable-transparent-visuals --disable-gpu .",
        "win32": "electron-packager . myApp --platform=win32 --arch=ia32",
        "win64": "electron-packager . myApp --platform=win32 --arch=x64",
        "linux32": "electron-packager . myApp --platform=linux --arch=ia32",
        "linux64": "electron-packager . myApp --platform=linux --arch=x64",
        "osx": "electron-packager . myApp --platform=darwin --arch=x64",
        "build": "electron-packager . --all",
        "webpack": "webpack --watch --webpack-config webpack.config.js"
    },
    "devDependencies": {
        "electron-prebuilt": "latest",
        "electron-packager": "^6.0.0",
        "socket.io": "*",
        "socket.io-client": "*",
        "jquery": "*",
        "animejs": "^2.2.0",
        "autosize": "^4.0.2",
        "babel": "^6.5.2",
        "babel-core": "^6.14.0",
        "babel-loader": "^6.2.5",
        "babel-preset-es2015": "^6.14.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.16.0",
        "bourbon": "^5.1.0",
        "css-loader": "^0.25.0",
        "extract-text-webpack-plugin": "^1.0.1",
        "node-sass": "^3.10.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-icons": "^2.2.7",
        "react-inlinesvg": "^0.7.4",
        "react-redux": "^5.0.6",
        "react-router": "^3.0",
        "react-router-transition": "^1.1.3",
        "react-transition-group": "^2.2.1",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "sass-loader": "^4.0.2",
        "style-loader": "^0.13.1",
        "superagent": "^3.8.2",
        "svg-inline-loader": "^0.7.1",
        "uglifyjs-webpack-plugin": "^1.1.5",
        "webpack": "^1.13.2",
        "webpack-bundle-tracker": "0.0.93",
        "webpack-livereload-plugin": "^0.8.2"
    }
}